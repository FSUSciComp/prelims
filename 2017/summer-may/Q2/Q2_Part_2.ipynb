   "source": [
    "F_function = @(x) exp(cos(x)); \n",
    "\n",
    "I = integral(F_function, 0, 2*pi);\n",
    "\n",
    "N   = 9;\n",
    "I_N = 0;\n",
    "\n",
    "for j = 1 : N\n",
    "    \n",
    "   x = (2 * pi * j) / N;\n",
    "   I_N = I_N + ((2*pi)/N) * (exp(cos(x)));\n",
    "    \n",
    "end\n",
    "\n",
    "Error = I_N - I;\n",
    "\n",
    "N\n",
    "I\n",
    "I_N\n",
    "Error "
   ]
  }
 ],
